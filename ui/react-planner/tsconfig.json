{
  "compilerOptions": {
    "jsx": "react-jsx",
    "sourceMap": true,
    "target": "es2019",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es5", "es6", "dom", "es2019"],
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "webpack"]
}
